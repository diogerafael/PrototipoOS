procedure TfrmPrincipal.ApplicationEvents1Exception(Sender: TObject;
  E: Exception);
    var
    NomeDoLog: string;
    Arquivo: TextFile;
begin
  Application.MessageBox('Verifique os Parametros Informados ao Sistema !',
    'Informação', MB_OK + MB_ICONINFORMATION);
    CriarLog(Sender,E,'Erro ');
end;

