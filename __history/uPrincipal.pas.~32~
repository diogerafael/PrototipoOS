procedure TfrmPrincipal.Alterarqtd_deslocamentoRural;
begin
try
    with datamodule1.comand do
    begin
      CommandText.Clear;
      CommandText.Add('ALTER TABLE os_engenharia_projeto_rural MODIFY COLUMN qtd_deslocamento  float(16,2)  DEFAULT NULL ');
      Execute;
    end;
  except
  end;
end;

